name        = DrupalCamp Austin
description = DrupalCamp Austin for Drupal 6
screenshot  = drupalcampaustin-preview.png
version     = 0.0 dev
core        = 6.x
engine      = phptemplate

; stylesheets
stylesheets[all][] = styles/reset.css
stylesheets[all][] = styles/grid.css
; stylesheets[all][] = styles/debug.css
stylesheets[all][]   = styles/style.css
; stylesheets[print][] = styles/style-print.css

; regions
regions[sidebar_1]     = "Sidebar 1"
regions[sidebar_2]     = "Sidebar 2"
regions[header]        = "Header"
regions[content]       = "Content"

; enabled features
features[] = name
features[] = slogan
features[] = breadcrumb
features[] = favicon
features[] = node_user_picture
features[] = comment_user_picture
features[] = search
features[] = primary_links
features[] = secondary_links

; disabled features (uncomment to activate)

; set directory where custom Panels layouts are located
plugins[panels][layouts] = panels_layouts